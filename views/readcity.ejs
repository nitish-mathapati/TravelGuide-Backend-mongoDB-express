<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cities</title>
</head>
<body>
    <h1>All Cities Are</h1>
    <div id="city-list">
        <% if (data && data.length > 0) { %>
            <% data.forEach(function(city) { %>
                <p><%= city.city_name %></p>
                <p><%= city.state %></p>
                <p><%= city.pincode %></p>
            <% }); %>
        <% } else { %>
            <p>No cities found.</p>
        <% } %>
    </div>
</body>
</html>